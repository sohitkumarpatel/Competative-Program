import java.util.*;
class ReverseArray
{
	static void reverse(int a[], int n)
	{
		int[] myArray=new int[n];
		int j=n;
		for(int i=0;i<n;i++)
		{
			myArray[j-1]=a[i];
			j=j-1;
		}
		System.out.println("Reverse the array:");
		for(int k=0;k<n;k++)
		{
			System.out.println(myArray[k]);
		}
	}
	
	public static void main(String[]args)
	{
		int[] arr= {10,20,30,40,50};
		reverse(arr, arr.length);
	}
}
