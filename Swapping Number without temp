import java.io.*;
import java.util.*;
class Swapping_Number
{
	public static void main(String[]args)
	{
		System.out.println("Enter the two number for swapping:");
		Scanner sc=new Scanner(System.in);
		int x=sc.nextInt();
		int y=sc.nextInt();
		System.out.println("Before Swapping Number: "+x+" and "+y);
		x=x+y;
		y=x-y;
		x=x-y;
		System.out.println("After the swapping Number: "+x+" and "+y);
	}
}
