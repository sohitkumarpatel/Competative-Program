import java.util.Scanner;
class ArraySum {
    static int sum(int arr[],int n) {
        // code here
       int  s=0;
        for(int num:arr)
        {
            s=s+num;
        }
        return s;
    }
    public static void main(String[]args)
    {
        Scanner sc=new Scanner(System.in);
        int N=sc.nextInt();
		int[] arr=new int[N];
        for(int i=0;i<N;i++)
        {
            arr[i]=sc.nextInt();
        }
        int result=sum(arr,N);
		
		
		System.out.println(result);
    }
      
}
